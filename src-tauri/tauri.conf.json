{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "GatorSec",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "execute": true,
        "sidecar": true,
        "open": true
      },
      "process": {
        "all": true
      },
      "path": {
        "all": true
      },
      "dialog": {
        "open": true
      },
      "fs": {
        "all": true,
        "createDir": true,
        "readFile": true,
        "writeFile": true,
        "scope": ["$APPDATA/logs/**", "$APPDATA/logs"]
      },
      "window": {
        "center": true
      }
    },
    "windows": [
      {
        "title": "GatorSec",
        "width": 1400,
        "height": 858,
        "x": 608,
        "y": 291
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.uf.filemodtracker",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "windows": {
        "wix": {
          "fragmentPaths": ["./windows/fragments/drivers.wxs"],
          "componentGroupRefs": ["DriverComponents"]
        }
      }
    }
  }
}